<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PIDEV.AddBlogController">
   <children>
      <ImageView fitHeight="80.0" fitWidth="78.0" layoutX="8.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/logo.png" />
         </image>
      </ImageView>
      <Label layoutX="120.0" layoutY="65.0" prefWidth="34.0" text="Titre:" textAlignment="CENTER" />
      <TextField fx:id="txttitre" layoutX="205.0" layoutY="55.0" prefHeight="37.0" prefWidth="184.0" />
      <Label layoutX="102.0" layoutY="146.0" prefHeight="17.0" prefWidth="68.0" text="Description:" textAlignment="CENTER" />
      <Label layoutX="113.0" layoutY="256.0" text="Contenu:" />
      <TextField fx:id="txtdesc" layoutX="206.0" layoutY="126.0" prefHeight="56.0" prefWidth="311.0" />
      <TextField fx:id="txtcontenu" layoutX="205.0" layoutY="223.0" prefHeight="100.0" prefWidth="330.0" />
      <Button fx:id="buttonadd" layoutX="242.0" layoutY="351.0" mnemonicParsing="false" onAction="#ajouter" text="Ajouter" />
      <Button fx:id="buttoncancel" layoutX="370.0" layoutY="351.0" mnemonicParsing="false" onAction="#annuler" text="Annuler" />
      
   </children>
</AnchorPane>
