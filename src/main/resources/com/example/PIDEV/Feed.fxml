<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="pane" prefHeight="770.0" prefWidth="1125.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PIDEV.FeedController">

   <top><fx:include source="header.fxml" BorderPane.alignment="TOP_CENTER" /></top>

   <center>
      <ScrollPane fx:id="scrollPane">
      <AnchorPane fx:id="feed" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
      </ScrollPane>
   </center>
   <bottom>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #F4F4F2;" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="filtre" layoutX="948.0" layoutY="14.0" />
            <Button fx:id="addOeuvre_btn" layoutX="55.0" layoutY="14.0" mnemonicParsing="false" onAction="#addOeuvre" text="Add Oeuvre" />
         </children></AnchorPane>
   </bottom>

</BorderPane>
