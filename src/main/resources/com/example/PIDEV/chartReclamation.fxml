<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.*?>

<AnchorPane fx:id="chartNode" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.PIDEV.chartRelamationController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <HBox fx:id="chartHBox" prefHeight="413.0" prefWidth="590.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox prefHeight="458.0" prefWidth="161.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#globalChart" prefHeight="26.0" prefWidth="143.0" text="Global">
                            <VBox.margin>
                                <Insets bottom="5.0" />
                            </VBox.margin>
                        </Button>
                        <MenuButton mnemonicParsing="false" prefHeight="26.0" prefWidth="142.0" text="Details">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#detailleOeuvre" text="Oeuvre" />
                                <MenuItem mnemonicParsing="false" onAction="#detailleEvent" text="Event" />
                                <MenuItem mnemonicParsing="false" text="Blogs" />
                                <MenuItem mnemonicParsing="false" text="Dons" />
                            </items>
                            <VBox.margin>
                                <Insets bottom="5.0" />
                            </VBox.margin>
                        </MenuButton>
                        <Button mnemonicParsing="false" onAction="#lineChart" prefHeight="26.0" prefWidth="141.0" text="Line Chart">
                            <VBox.margin>
                                <Insets bottom="5.0" />
                            </VBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#convertirPDF" prefHeight="26.0" prefWidth="144.0" text="Convertir PDF   ">
                            <VBox.margin>
                                <Insets top="300.0" />
                            </VBox.margin></Button>
                    </children>
                </VBox>
                <AnchorPane  prefHeight="414.0" prefWidth="498.0" />
            </children>
        </HBox>
    </children>

</AnchorPane>
